generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

enum TransactionType {
  INCOME
  EXPENSE
}

model Transaction {
  id   Int             @id @default(autoincrement())
  name String
  date DateTime
  type TransactionType

  createdAt DateTime @default(now())
  updatedAt DateTime
}
